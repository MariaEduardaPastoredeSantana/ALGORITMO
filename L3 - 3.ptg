algoritmo "questao"

var cont : inteiro
var x : real

inicio

cont <- 0

repita 
   escreval("digite o numero (digite 0 pra encerrar)")
   leia(x)
      se (x > 100) e (x < 200) entao
         cont <- cont+1
   fimse
ate x = 0

escreval("qtd de numeros maiores que 100 e menores que 200 = ", cont)

fimalgoritmo
