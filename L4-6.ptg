algoritmo "semnome"

var

mat : vetor[1..2, 1..2] de inteiro
l, c, num, max, min : inteiro

inicio

para l de 1 ate 2 faca
     para c de 1 ate 2 faca
     escreval("digite o numero: ")
     leia(mat[l,c])
     se l+c = 3 entao
        mat[l,c] <-
        escreval("este numero está na diagonal secundária", mat[l,c])
        escreval(" ")
     fimse
     fimpara
fimpara

fimalgoritmo
