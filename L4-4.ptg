algoritmo "semnome"

var

mat : vetor[1..2, 1..2] de inteiro
l, c, num : inteiro

inicio

para l de 1 ate 2 faca
     para c de 1 ate 2 faca
     escreval("digite o numero: ")
     leia(mat[l,c])
     fimpara
fimpara

para l de 1 ate 2 faca
     para c de 1 ate 2 faca
     se (mat[l,c] MOD 2) <> 0 entao
     escreva(" ", mat[l,c])
     fimse
     fimpara
fimpara

fimalgoritmo
